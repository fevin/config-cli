[alias]
    st = status
    co = checkout
    ci = commit
    lg = log
    br = branch
    d  = difftool -y
    m  = mergetool -y
[core]
    editor = vim

[diff]
    tool = vimdiff
    prompt = false

[merge]
    tool = vimdiff
    prompt = false
[difftool]
    prompt = false
